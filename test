import telebot
import webbrowser

bot = telebot.TeleBot("Your token")

@bot.message_handler(commands=['download'])
def handle_message(message):
    url_video = message.text.split(" ")[1] # Get user input
    webbrowser.open("https://ytdownload.com/download?video=" + url_video + "&formats=best&quality=144p") # Open page with download
    bot.send_message(message.chat.id, "Done. Video ready to be downloaded") # Send response
